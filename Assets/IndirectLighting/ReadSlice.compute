#pragma kernel CSMain

RWStructuredBuffer<uint> Result;
RWTexture3D<uint> Source;
int VoxelResolution;

[numthreads(8,8,8)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
    Result[
        id.x +
        id.y * VoxelResolution +
        id.z * (VoxelResolution * VoxelResolution)
    ] = Source[id];
}
